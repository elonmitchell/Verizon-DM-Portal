.pm-link {
	cursor: pointer;
}

#popupviewer-popup-wrap {
}

.modal-content {
	border-radius: 0;
	max-width: 960px;
}

.modal-body-content-center {
	text-align: center;
}


/* fancybox popups */
.fancybox-popup-wrap {
	display: none;
	font-size: 14px;
}

.fb-popup {
	position: relative;
	border: solid #cccccc;
	border-width: 4px;
	padding: 20px 20px 23px 24px;
	background: #eeeeee;
	overflow: hidden;
}

.fb-popup .error-message {
	text-align: center;
	font-size: 14px;
	padding: 4px;
	border: 2px red solid;
	background: #ffeeee;
	max-width: 96%;
	margin: auto;
}

.fb-popup h1 {
	margin-bottom: 8px;
	text-align: center;
	white-space: nowrap;
}

.fb-popup p {
	margin: 20px 0 12px 0;
	font-size: 14px;
	line-height: 20px;
}

.fb-popup ul {
	list-style: disc outside none;
	margin: 0 0 18px;
	font-size: 16px;
}

.fb-popup ul li {
	margin-left: 20px;
	font-size: 14px;
}

.fb-popup table tr td {
	padding: 10px 5px 0px 5px;
	font-size: 14px;
}

.fb-popup table tr.error {
	display: none;
}

.fb-popup table tr.relatedRow td {
	padding-top: 5px;
}

.fb-popup table tr.error td {
	padding-top: 0;
	padding-bottom: 0px;
}

.fb-popup .popup-submit-button {
	margin-top: 20px;
	text-align: center;
}

.fb-popup .popup-submit-button-left { 
	float: left;
}

.fb-popup .popup-submit-button-right { 
	float: right;
	margin-left: 5px;
}

.fb-popup .popup-submit-button-clear {
	clear: both;
}

.fb-popup tr.newSection td {
	padding-bottom: 4px !important;
}
.fb-popup tr.newSection fieldset {
	border-top: 1px #ccc solid;
}
.fb-popup tr.newSection fieldset legend {
	font-size: 14px;
	padding: 0 16px;
	font-style: italic;
}

.formLabel {
}

.formLabel .reqd {
	color: #CC0000;
}

.formFieldError {
	color: #CB2027;
	font-size: 0.9em;
}

.formFieldErrorSmall {
	font-size: 11px;
	color: #890000;
}

.meridianGroup {
	font-size: 10px;
}

.cstNote {
	margin-top: 10px;
	text-align: center;
	font-size: 12px;
}

.labelNote {
	text-align: right;
	font-size: 12px;
}

/* ********************* */
/* Upload campaign popup */
/* ********************* */

#upload-software-popup { 
	width: 450px;
}

.upload-software-popup-wait, .popup-wait {
	padding: 25px 0 15px 0;
	text-align: center;
	font-size: 13px;
}

.upload-software-popup-model-wait {
	display: inline;
}

.upload-software-popup-time-notice {
	padding-top: 15px;
	padding-bottom: 5px;
}

div.scrollable {
	border: 1px solid #ccc;
	background: #ffffff;
	padding: 0 4px;
	overflow: auto; 
	max-height: 90px;
	display: block;
}

div.scrollable table {
	width: 100%;
}

div.scrollable table tr td {
	font-size: 11px;
	padding: 2px 6px 2px 0;
}

div.scrollable hr {
	border: none;
	background-color: #ccc;
	color: #ccc;
	height: 1px;
	margin-top: 0;
	margin-bottom: 0;
}


div.scrollingDivArea {
	border: 1px solid #aaa;
	background: #ffffff;
}

div.scrollingDivArea div.scrollingDivHeader {
	padding: 0 4px;
	background: #eee;
	border-bottom: 1px solid #aaa;
}

div.scrollingDivArea div.scrollingDiv {
	padding: 0 4px;
	overflow: auto;
	max-height: 70px;
	display: block;
}

div.scrollingDivArea div.scrollingDivHeader table,
div.scrollingDivArea div.scrollingDiv table {
	width: 100%;
}

div.scrollingDivArea div.scrollingDivHeader table tr td,
div.scrollingDivArea div.scrollingDiv table tr td {
	font-size: 11px;
	padding: 2px 6px 2px 0;
}

div.scrollingDivArea div.scrollingDiv hr {
	border: none;
	background-color: #ccc;
	color: #ccc;
	height: 1px;
}


