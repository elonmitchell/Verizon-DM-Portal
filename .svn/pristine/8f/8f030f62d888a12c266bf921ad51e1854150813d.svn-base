/* ******************* */

#add-blacklist-device-popup {
	width: 420px;
	min-height: 200px;
}
table.blacklist-manual-table {
	width: 100%;
	border-width: 1px;
	border-spacing: 2px;
	border-style: solid;
	border-color: #999999;
	border-collapse: collapse;
}
table.blacklist-manual-table th,
table.blacklist-manual-table td {
	border-width: 1px;
	padding: 2px;
	border-style: solid;
	border-color: #999999;
	font-weight: normal;
	width: 50%;
}

table.blacklist-manual-header-table,
table.blacklist-manual-header-table th {
	border-bottom-width: 0;
}

