.dashboard-chart .style_10 { border-bottom: thin solid rgb(0, 0, 0);}
.dashboard-chart .style_12 { font-family: Calibri; font-style: normal; font-variant: normal; font-weight: normal; font-size: 9pt; color: rgb(31, 73, 125); margin: 0pt; padding: 1pt; border: medium none black; letter-spacing: normal; word-spacing: normal; text-transform: none; white-space: normal; line-height: normal;}
.dashboard-chart .style_11 { font-family: Calibri; font-style: normal; font-variant: normal; font-weight: bold; font-size: 9pt; color: black; margin: 0pt; padding: 1pt; border: medium none black; letter-spacing: normal; word-spacing: normal; text-transform: none; white-space: normal; line-height: normal;}


table#dashboard-historical-reports-data-table th { text-align: left; }
table#dashboard-historical-reports-data-table td.cell-date { text-transform: uppercase; }

.dashboard-more-details-area {
  text-align: center;
  font-size: 11px;
  padding-top: 4px;
}

.dashboard-data-table .campaign-stats-chart { padding: 0 10px; }


.dataListDiv { margin-top: 20px; }
.dataListDiv table tr th { background-color: #dfbbbb; }
.dataListDiv table tr td, .dataListDiv table tr th {
  border: 1px solid #999999;
  padding: 4px;
}
.dataListDiv table tr.oddRow { background-color: #f1f1f1; }
.dataListDiv table tr.evenRow { background-color: #e1e1e1;}
.dataListDiv table tr.oddRow.newRow { background-color: #feffe4; }
.dataListDiv table tr.evenRow.newRow { background-color: #F2F3C5; }
.dataListDiv table tr.newRow .newIcon {
    background: url('../images/new_report.png') no-repeat;
    width: 23px;
    height: 22px;
    position: absolute;
    bottom: 5px;
    left: 2px;
}

.dataListDiv h3 {
  margin-bottom: 10px; font-size: 16px;
}

.dataListDiv table tr td.layoutLeftCell, .dataListDiv table tr td.layoutRightCell { border: 0; padding: 0; vertical-align: top; }
.dataListDiv table tr td.layoutLeftCell { border: 0; padding-right: 6px; }
.dataListDiv table tr td.layoutRightCell { border: 0; padding-left: 6px; }
/* .dataListDiv table tr td.borderRightCell { border-left: 1px solid #cccccc; } */


table.data_table tr th { background: #dfbbbb; }
table.data_table tr td, table.data_table tr th {
  border: 1px solid #999999;
  padding: 4px;
}
table.data_table tr.odd_table_row td { background: #f1f1f1; }
table.data_table tr.even_table_row td { background: #e1e1e1;}

table.data_table tr td.layoutLeftCell, table.data_table tr td.layoutRightCell { border: 0; padding: 0; vertical-align: top; }
table.data_table tr td.layoutLeftCell { border: 0; padding-right: 6px; }
table.data_table tr td.layoutRightCell { border: 0; padding-left: 6px; }
/* table.data_table tr td.borderRightCell { border-left: 1px solid #cccccc; } */

.table_div { border: none;  }


table#viewReportsTable tr th, table#viewReportsTable tr td {
  font-size: 12px;
}

table#historicalReportsTable { width: 100%; }

table#historicalReportsTable tr th, table#historicalReportsTable tr td {
  font-size: 12px;
  border: 0;
  padding-top: 4px;
  padding-bottom: 4px;
  padding-left: 8px;
  padding-right: 0px;
}
table#historicalReportsTable tr th {
  background: #bbc7df;
  font-weight: normal;
}
table#historicalReportsTable tr td {
  padding-top: 6px;
  padding-bottom: 6px;
}

.historicalReportsSubline {
  font-size: 11px;
}

#historical-reports-fieldset legend {}

td.cb { text-align: center; }

.popup-submit-button-left { float: left; padding-right: 10px; }

.img-button-medium2 { padding: 5px 15px; cursor: pointer; }

#dashboard-slider .slide {
	padding-left: 2px;
	padding-right: 2px;
}

/* New styles for dashboard calendar */

