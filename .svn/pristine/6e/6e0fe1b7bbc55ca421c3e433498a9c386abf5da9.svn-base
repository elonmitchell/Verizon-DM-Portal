/* CSS Document */ 
*{padding:0;margin:0;}
html, body {height: 100%; margin:0;padding:0; position:relative; }
*html{overflow-x:auto; overflow-y:scroll;}
body{font-family:Tahoma,Arial,Helvetica,sans-serif; font-size:0.7em; color:#000;background:url(../images/background.gif) repeat-x top #DAD8D8;}
body.modaldialog{background-color:#E1E1E1; background-image:none;}
ul{list-style:none;}
/*
form{margin:0 1% 1% 1%; padding:1%; font-size:1em; font-weight:bold; background-color:#E1E1E1; border:1px solid #919090; border-top:none;}
form a{font-weight:normal; color:#06F; text-decoration:underline;}
*/
input{font-family:Tahoma,Geneva,sans-serif; font-size:1em; line-height:18px; margin:3px 0 3px 0; font-weight:normal;}
select{font-family:Tahoma,Geneva,sans-serif; font-size:1em; line-height:18px; margin:0pt;}

input.textbox{font-family:Tahoma,Geneva,sans-serif; font-size:1em; line-height:18px; margin:2px 0 2px 10px; padding:0 0 0 2px; height:15px; width:224px;}
select.selectbox{font-family:Tahoma,Geneva,sans-serif; font-size:1em; line-height:18px; margin:2px 0 2px 2px; padding:0 0 0 2px; height:19px; width:206px;}
select.selectbox2{font-family:Tahoma,Geneva,sans-serif; font-size:1em; line-height:18px; margin:2px 0 2px 10px; padding:0 0 0 2px; height:19px; width:230px;}
textarea.firmware{font-family:Tahoma,Geneva,sans-serif; font-size:1em; line-height:18px; margin:2px 0 2px 10px; padding:0 0 0 2px; width:230px;}

input.btn{margin:0; padding:0 0 0 2px; height:18px; cursor:pointer; border:none;}
input.radio{padding:0pt; margin:0 1em 0 1.75em;}
input.radio2{padding:0pt; margin:0 1em 0 1em;}
input.checkbox{padding:0pt; margin:0 1em 0 1em;}
a:link{color:#06F; text-decoration:underline;}
table{font-size:1em;   border-collapse:collapse; font-family:tahoma,arial,verdana,helvetica,sans-serif;}
legend{ margin-left:7px;color:inherit; *margin-left:0px; *color:#333;}
#wrapper{margin:0 auto; width:100%;}
/* Commented Backslash Hack hides rule from IE5-Mac \*/#wrapper{min-width:1000px !important}

#header{width:100%; background:#890000; height:85px;color:white;} /* url("nav_bg_sprite.gif") 0 -55px repeat-x  */
#header a { color:white; }
.header-center { width: 985px; margin-left:auto; margin-right:auto; }
.shadow{width:100%; background:url(../images/shadow.gif) repeat-x top; padding:0pt; margin:0pt;}
#header .header-product-title{float:left;width:63%;font-size:165%;font-weight:bold;display:table; height: 100%; position: relative; overflow: hidden;color:#047;margin-left:10px; margin-top: 18px;}
#header .header-product-title div.cell {float: left; display: table-cell; vertical-align: middle;  width: auto;}
#header .header-product-title div.cell p {*position: relative; top: -50%;}
#header .header-logo{padding-right:5px;height:65px;text-align:right;}
.header_right{float:right;width:35%;}
#navig{WIDTH:98%; margin:1% 1% 0 .55%; padding:0; HEIGHT:33px; float:left;}

.loginpage #header{width:100%; background:#890000; height:35px;color:white;} /* url("nav_bg_sprite.gif") 0 -55px repeat-x  */
.loginpage #header .header-product-title{float:left;width:73%;font-size:65%;font-weight:bold;display:table; height: 35px; position: relative; overflow: hidden;color:#047;margin-left:10px; margin-top: 0px;}
.loginpage #header .header-product-title div.cell {*position: absolute; top: 0; display: table-cell; vertical-align: middle;}
.loginpage #header .header-logo{padding-right:5px;height:35px;text-align:right;}
.loginpage .header_right{float:right;width:15%;}
/** START TABS **/

#tabs {
  font-family: Tahoma,Arial,Helvetica,sans-serif;
  width: 982px;
  font-size: 14px;
  font-weight: bold;
  line-height: 20px;
  min-width: 982px !important;
  margin-left: auto;
  margin-right: auto;
}

* html #tabs{ margin-bottom: 1em; } /*IE only. Add 1em spacing between menu and rest of content*/

#tabs ul {
  list-style-type: none;
  margin: 0;
  margin-left: 10px;
  padding: 0;
}

#tabs ul li {
  float: left;
  margin: 0;
  margin-right: 2px;
  padding: 0;
}

#tabs a {
  float: left;
  background: url("../images/tabs/tab-left.gif") no-repeat left top;
  margin: 0;
  padding: 0 0 0 9px;
  text-decoration: none;
}

#tabs a span {
  float: left;
  display: block;
  background: url("../images/tabs/tab-right.gif") no-repeat right top;
  padding: 6px 25px 7px 20px;
  font-weight: bold;
  color: #fff;
}

/* Commented Backslash Hack hides rule from IE5-Mac \*/
#tabs a span {float:none;}
/* End IE5-Mac hack */

#tabs a:hover { background-position: 0% -125px; }
#tabs a:hover span { background-position: 100% -125px; color: #fff; }
#tabs li.selectedmenu a { background: url("../images/tabs/tab-left-on.gif") no-repeat left top; }
#tabs li.selectedmenu a span { background: url("../images/tabs/tab-right-on.gif") no-repeat right top; color: #000; }

/** END TABS **/

