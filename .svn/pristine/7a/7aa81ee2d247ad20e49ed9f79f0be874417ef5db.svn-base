<div class="form-group {{#if fieldData.relatedAbove === true}}form-group-related-above{{/if}}">
	{{#fieldData.name}}<label for="{{fieldId}}">{{fieldData.name}}</label>{{/fieldData.name}}
	<div class="fieldValue {{fieldId}}">
		{{# {bChecked: fieldData.value === fieldData.elementValue, className: (fieldData.relatedAbove || fieldData.inline == true) ? "checkbox-inline" : "checkbox"} }}
			<div>
				<input type="checkbox" class="{{className}}" id="{{fieldId}}" {{> field-attributes}} value="{{fieldData.elementValue}}" {{#if bChecked}}checked="on"{{/if}}>
				{{#fieldData.valueLabel}}
					<label for="{{fieldId}}" class="value-label">{{fieldData.valueLabel}}</label>
				{{/fieldData.valueLabel}}
			</div>
		{{/}}
	</div>
	{{> help-blocks}}
</div>


